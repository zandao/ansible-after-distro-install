*.{tar.gz,zip} filter=lfs diff=lfs merge=lfs -text
*.{png.jpg} filter=lfs diff=lfs merge=lfs -text
*.{ttf,ttx} filter=lfs diff=lfs merge=lfs -text
*.otf !text !filter !merge !diff
*.gz !text !filter !merge !diff
*.sh !text !filter !merge !diff
*.PFB !text !filter !merge !diff
*.png !text !filter !merge !diff
*.jpg !text !filter !merge !diff
